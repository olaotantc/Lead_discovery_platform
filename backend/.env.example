# Server Configuration
# Server Configuration
PORT=8000
HOST=0.0.0.0
NODE_ENV=development
FRONTEND_URL=http://localhost:3000

# CORS Configuration
# Comma-separated origins. Set to * or set CORS_ALLOW_ALL=true to allow any origin (not recommended outside dev)
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
CORS_ALLOW_ALL=false

# Auth
JWT_SECRET=change-me-min-32-chars
JWT_EXPIRY=7d

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/lead_discovery
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=lead_discovery

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379

# API Keys (from main .env)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
PERPLEXITY_API_KEY=

# AI Model Configuration
# ICP_MODEL: Model for ICP inference (default: gpt-4o-mini)
# Options: gpt-4o-mini, gpt-5-mini, gpt-4o
ICP_MODEL=gpt-4o-mini

# Email Verification
BOUNCER_API_KEY=
NEVERBOUNCE_API_KEY=

# Email Discovery Providers
HUNTER_API_KEY=
CLEARBIT_API_KEY=

# Account Discovery Providers (Lookalikes)
CLEARBIT_DISCOVERY_API_KEY=
BING_SEARCH_API_KEY=
BING_SEARCH_ENGINE_ID=

# Email Integration
GMAIL_CLIENT_ID=
GMAIL_CLIENT_SECRET=
OUTLOOK_CLIENT_ID=
OUTLOOK_CLIENT_SECRET=

# Sequencer Integration
SMARTLEAD_API_KEY=
INSTANTLY_API_KEY=
